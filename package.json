{
  "name": "telechart",
  "version": "1.0.0",
  "description": "Minimalistic charts without dependencies",
  "source": "./src/module.js",
  "global": "./dist/global/telechart.min.js",
  "main": "./dist/cjs/telechart.js",
  "module": "./dist/esm/telechart.js",
  "targets": {
    "global": {
      "context": "browser",
      "outputFormat": "global",
      "optimize": true,
      "source": [
        "./src/browser.js"
      ]
    },
    "main": {},
    "module": {}
  },
  "scripts": {
    "start": "parcel example/index.html",
    "build": "parcel build",
    "serve": "npx http-server demo --gzip -o"
  },
  "browserslist": "> 0.25%, not dead",
  "keywords": [
    "chart",
    "canvas"
  ],
  "author": "vladislaosan@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "parcel": "^2.10.3",
    "prettier": "3.1.0"
  }
}
